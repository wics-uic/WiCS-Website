---
import astroLogo from '../assets/astro.svg';
import background from '../assets/background.svg';
import cloud from '../assets/extra/cloud.svg';
import onestar from '../assets/extra/onestar.svg';
import twostar from '../assets/extra/twostars.svg';
import curve from '../assets/bg-curve.png';


---

<!-- <wave/> -->
<section id="container" class="relative flex flex-col gap-10 w-full justify-center items-center h-screen bg-pink-50 overflow-hidden">
	<!-- background images -->
	<img
		src={cloud.src}
		alt="left cloud"
		class="absolute w-[800px] bottom-[-20px] left-[-250px] z-0"
	/>
	<img
		src={cloud.src}
		alt="right cloud"
		class="absolute w-[800px] top-[0%] right-[-250px] z-0"
	/>
	<img
		src={onestar.src}
		alt="one star"
		class="absolute w-[25px] bottom-[300px] right-[400px] z-0"
	/>
	<img
		src={twostar.src}
		alt="two stars"
		class="absolute w-[30px] top-[230px] left-[390px] z-0"
	/>

